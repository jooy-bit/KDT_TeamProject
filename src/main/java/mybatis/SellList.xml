<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="SellList">
 	<select id="getList" resultType="dto.SellList" 
 			parameterType="dto.Paging" >
 		SELECT * FROM 
			KDT_SELLLIST WHERE CATEGORY = #{category}
 	</select>
 	<select id="count" resultType="int" 
 			parameterType="java.util.Map" >
 		SELECT count(*) 
		KDT_SELLLIST WHERE CATEGORY = #{category}
 	</select>
 	<select id="getOne" parameterType="int" resultType="dto.SellList">
 		SELECT * 
		FROM KDT_SELLLIST
		WHERE PDT_CODE = #{PDT_CODE} 
 	</select>
 </mapper>
 
 
 
 
 
 